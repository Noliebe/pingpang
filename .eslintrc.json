{
    "root": true,
    "env": {
        "browser": true,
        "es6": true
        // "es2021": true
    },
    "extends":
     [
        // "airbnb-base",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        // "plugin:@typescript-eslint/parser",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
    //     "ecmaVersion": "latest"
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        
        "prettier"
    ],
    "rules": {
        // prettier
        "prettier/prettier": "error"
    }
  }
  